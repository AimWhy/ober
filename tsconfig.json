{
  "compilerOptions": {
    "outDir": "./lib/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "allowJs": true,
    "alwaysStrict": false
  },
  "exclude": [
    "./lib",
    "./node_modules"
  ],
  "include": [
    "./src/**/*.ts",
    "./src/**/*.js"
  ]
}